{
  "name": "awesome-swiper",
  "version": "1.4.0",
  "description": "awesome-swiper based on swiper",
  "main": "build/AwesomeSwiper.js",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=development webpack --display-modules --display-reasons --display-error-details --watch --color",
    "build": "cross-env NODE_ENV=pack webpack --progress --display-modules --display-reasons --display-error-details --color && cross-env NODE_ENV=production webpack --progress",
    "build:dev": "cross-env NODE_ENV=development webpack --display-modules --display-reasons --display-error-details --color",
    "pack:standalone": "cross-env NODE_ENV=standalone webpack --display-modules --display-reasons --display-error-details --color",
    "pack:all": "npm run build:dev && npm run build && npm run pack:standalone && gulp copy:demo && gulp copy:build",
    "deploy": "gulp deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycdpo/awesome-swiper.git"
  },
  "keywords": [
    "swiper"
  ],
  "author": "cycjimmy <cycjimmy@gmail.com> (https://github.com/cycjimmy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cycdpo/awesome-swiper/issues"
  },
  "homepage": "https://github.com/cycdpo/awesome-swiper#readme",
  "dependencies": {
    "@cycjimmy/awesome-js-funcs": "^1.0.1",
    "@cycjimmy/sass-lib": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "autoprefixer": "^9.6.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "gh-pages": "^2.1.1",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "swiper": "^5.0.4",
    "uglify-js": "^3.6.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9"
  }
}
