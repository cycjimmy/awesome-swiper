  "description": "awesome-swiper based on swiper",
  "main": "build/awesome-swiper.min.js",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=development webpack --display-modules --display-reasons --display-error-details --watch --color",
